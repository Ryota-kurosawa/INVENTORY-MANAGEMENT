	xmlns:b="urn:import:com.github.gwtbootstrap.client.ui"
	xmlns:p1="urn:import:com.google.gwt.user.cellview.client">
	<ui:style>
		.important {
			font-weight: bold;
		}
	</ui:style>
	<g:HTMLPanel>
	<g:LayoutPanel width="100%" height="100%" ui:field="layoutpanel">
			<g:layer left="116px" width="210px" top="205px" height="32px">
				<b:Button text="送信" ui:field="btn_Send" type="PRIMARY" icon="CHECK"/>
			</g:layer>
			<g:layer left="0px" width="450px" top="25px" height="2px">
				<b:Image url="img/line_1260.png"/>
			</g:layer>
			<g:layer left="5px" width="148px" top="3px" height="20px">
				<!-- <b:Label text="アンケート結果送信" styleName="krsw-Label"/> -->
				<b:Badge type="INFO" horizontalAlignment="ALIGN_CENTER">アンケート結果送信</b:Badge>
			</g:layer>
			<g:layer left="0px" width="450px" top="276px" height="2px">
				<b:Image url="img/line_1260.png"/>
			</g:layer>
			<g:layer left="5px" width="60px" top="55px" height="20px">
				<b:Label text="所属部門" styleName="krsw-Label"/>
			</g:layer>
			<g:layer left="5px" width="60px" top="129px" height="20px">
				<b:Label text="氏名" styleName="krsw-Label"/>
			</g:layer>
			<g:layer left="5px" width="439px" top="148px" height="56px">
				<b:TextBox width="425px" height="30px" ui:field="txtbox_Name"/>
			</g:layer>
			<g:layer left="214px" width="32px" top="215px" height="32px">
				<b:Image url="img/loadingSpinMINI.gif" ui:field="img_Spin"/>
			</g:layer>
			<g:layer left="116px" width="229px" top="250px" height="20px">
				<b:Label text="送信完了しました。お疲れ様でした。" ui:field="lbl_End" styleName="krsw-Label"/>
			</g:layer>
			<g:layer left="5px" width="440px" top="72px" height="43px">
				<b:ListBox width="440px" height="30px" ui:field="cmb_searchBusho"/>
			</g:layer>
		</g:LayoutPanel>
	</g:HTMLPanel>
</ui:UiBinder>